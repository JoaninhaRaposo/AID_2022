        </Level>
        <Level name="To Airport City" visible="true" column="to_airport_city" type="String" uniqueMembers="false" levelType="Regular">
        </Level>
        <Level name="To Airport Name" visible="true" column="to_airport_name" type="String" uniqueMembers="false" levelType="Regular">
        </Level>
      </Hierarchy>
