    </Table>
    <Dimension type="StandardDimension" visible="true" foreignKey="airline_id" name="Airline">
      <Hierarchy name="Airline Hierarchy" visible="true" hasAll="true" allMemberName="All Airlines" primaryKey="airline_id">
